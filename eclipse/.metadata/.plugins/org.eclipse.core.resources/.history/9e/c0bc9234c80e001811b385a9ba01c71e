package com.kaiblok.testmod.blocks;

import net.minecraft.block.material.Material;



public class Diamond2Block extends BlockBase 
{
	public static final String mName = "diamond_block2";
	public static final Material mMaterial = Material.IRON;

	public Diamond2Block() 
	{
		super(mName, mMaterial);
		setLightLevel(1.0F);
		setLightOpacity(7);
	}

}
